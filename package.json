{
  "name": "yarny-app",
  "version": "2.0.0",
  "description": "Yarny React migration workspace with Google Drive integrations and Netlify Functions",
  "author": "Lindsay Brunner",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:functions": "netlify dev",
    "build": "npm run build:functions && vite build",
    "build:functions": "tsc -p tsconfig.functions.json",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.functions.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@netlify/blobs": "^10.3.2",
    "@tanstack/query-core": "^5.59.0",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-virtual": "^3.0.0",
    "@tiptap/core": "^2.2.6",
    "@tiptap/extension-document": "^2.2.6",
    "@tiptap/extension-hard-break": "^2.2.6",
    "@tiptap/extension-history": "^2.2.6",
    "@tiptap/extension-paragraph": "^2.2.6",
    "@tiptap/extension-text": "^2.2.6",
    "@tiptap/react": "^2.2.6",
    "axios": "^1.13.1",
    "google-auth-library": "^9.0.0",
    "googleapis": "^128.0.0",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.5",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.4",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^2.1.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "msw": "^2.5.1",
    "netlify-cli": "^17.0.0",
    "postcss": "^8.4.48",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  }
}
